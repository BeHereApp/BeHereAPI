{
  "name": "BeHereAPI",
  "version": "1.0.0",
  "description": "RESTful API for BeHere application",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "git@github.com:BeHereApp/BeHereAPI.git",
  "author": "Elton Viana <eltonvs@outlook.com>",
  "contributors": [
    "Thais Lins <thaislin@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "http-status": "^1.0.1",
    "mongoose": "^4.13.5",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.12.1",
    "nodemon": "^1.12.1"
  }
}
